{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"esModuleInterop": true,
		"baseUrl": ".",
		"paths": {
			"@assets/*": ["assets/*"],
			"@components/*": ["src/components/*"],
			"@store/*": ["src/rematch/*"],
			"@screens/*": ["src/screens/*"]
		}
	},
	"include": ["src/**/*"]
}
